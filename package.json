{
  "name": "learn-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "vite --host",
    "build": "vue-tsc && npx tailwindcss -i ./src/style/index.css -o ./src/style.css && vite build",
    "preview": "vite preview",
    "dev:tailwind": "npx tailwindcss -i ./src/style/index.css -o ./src/style.css --watch"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "vite-tsconfig-paths": "^4.0.8",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
